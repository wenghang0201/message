# 环境
NODE_ENV=development

# 数据库 (MySQL - 与 client_api 共享)
DB_HOST=localhost
DB_PORT=3306
DB_USERNAME=root
DB_PASSWORD=
DB_DATABASE=chat_client_db

# JWT - 必须与 client_api 的 JWT_SECRET 匹配!
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production-min-32-chars

# 加密 - 安全关键 (生产环境请修改!)
ENCRYPTION_KEY=your-32-character-secret-key-here!
