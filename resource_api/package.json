{
  "name": "caipiap_backend_api",
  "version": "3.0.0",
  "description": "",
  "main": "App.js",
  "scripts": {
    "start": "ts-node src/App.ts",
    "dev": "ts-node-dev --respawn --transpile-only src/App.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/data-source.ts",
    "migration:create": "typeorm-ts-node-commonjs migration:create",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/data-source.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/data-source.ts",
    "migration:show": "typeorm-ts-node-commonjs migration:show -d src/data-source.ts",
    "package_win": "pkg build/App.js --output ./caipiap_backend_api --target node14-win-x64",
    "package_linux": "pkg build/App.js --output ./caipiap_backend_api --target node14-linux-x64"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/jsonwebtoken": "^9.0.10",
    "axios": "^0.27.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^4.18.2",
    "js-md5": "^0.8.3",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.14.1",
    "pkg": "^5.8.1",
    "protobufjs": "^7.2.5",
    "reflect-metadata": "^0.2.2",
    "request-payload": "^1.0.1",
    "ts-node": "^10.9.1",
    "typeorm": "^0.3.27",
    "urlencode": "^2.0.0",
    "websocket": "^1.0.34",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.21",
    "@types/websocket": "^1.0.6",
    "ts-node-dev": "^2.0.0"
  },
  "bin": {
    "trade_double": "build/App.js"
  },
  "pkg": {
    "scripts": "build/**/*.js",
    "outputPath": ".",
    "targets": [
      "node14-linux-x64",
      "node14-win-x64"
    ],
    "assets": [
      "node_modules/telegram/tl/static/api.tl",
      "node_modules/telegram/tl/static/schema.tl"
    ]
  }
}
